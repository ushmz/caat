{
  "manifest_version": 3,
  "version": "0.0.1",
  "short_name": "caat",
  "name": "Copy as anchor text",
  "description": "Add context menu to copy link as anchor text.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "contextMenus", "scripting"],
  "host_permissions": ["https://*/*"],
  "default_locale": "en"
}
